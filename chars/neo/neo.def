st      = the_neo.cns   ;キャラのプログラム
st1     = -2.cns
st2     = -3.cns
st3     = jump.cns
st4     = striker.cns
st5     = away.cns
st6     = dash.cns
;st6     = common.cns
;st99     = power.cns
stcommon = common1.cns 
sprite  = the_neo.sff   ;キャラの絵
anim    = the_neo.air   ;キャラの動き
sound   = the_neo.snd   ;キャラの音
pal1    = the_neo.act  ;キャラの色（最大１２個）
pal2    = pal2.act  ;キャラの色（最大１２個）
