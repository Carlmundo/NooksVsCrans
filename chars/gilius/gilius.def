; Definition file for Player
; Contains all the filenames needed for the character
; Template player 9X-09-12

; Character information
[Info]
name    = "Gilius Thunderhead";<-- Replace with a real name
displayname = "Gilius";<-- Replace with name to display
versiondate = 07,31,2000    ;Version date of character
mugenversion = 09,23,2000   ;Version of M.U.G.E.N character works on
author = "Foxtlot"         ;Character author name
pal.defaults = 1     ;Default palettes in order of preference


; Files for the character
[Files]
cmd     = gilius.cmd     ;Command set
cns     = gilius.cns     ;Constants
st      = gilius.st     ;States
st2     = gilius.st2     ;States
st3     = gilius.st3     ;States
stcommon = common1.cns   ;Common states
sprite  = gilius.sff     ;Sprite
anim    = gilius.air     ;Animation
sound   = gilius.snd     ;Sound
pal1    = gilius.act     ;Palettes
pal2    = gilius2.act     ;Palettes

