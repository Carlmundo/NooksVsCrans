;---------------------------------------------------------------------------
; Jump Start
;---------------------------------------------------------------------------
[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1

;Prevents characters from cloning due to reversal def moves you never know 0___0
[State DestroyHelper]
type=destroyself
trigger1=ishelper
ignorehitpause=1
pausemovetime=9999999
supermovetime=9999999

[State 40, 1]
type = null;AfterImage
triggerall = !time
trigger1 = prevstateno = 100
trigger2 = prevstateno = 11
trigger3 = prevstateno = 12
trigger4 = prevstateno = 10
time 		= 15
timegap		= 2
framegap 	= 2
length 		= 10
paladd 		= 0, 0, 0
palmul 		= .50, 0, .50
palcontrast 	= 100,50,150
trans 		= add

[State 40, 2]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 40, 2]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, 4]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 40, 5]
type = VelSet
trigger1 = AnimTime = 0
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x), const(velocity.jump.back.x)))
y = const(velocity.jump.y)

[State 40, 6]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = prevstateno = 100 ;RUN_FWD
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)
y = const(velocity.jump.y) - 2

[State 40, End]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1

