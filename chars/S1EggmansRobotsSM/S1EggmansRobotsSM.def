sprite  = ZoneS1.sff   ;Sprite
anim    = ZoneS1.air.txt   ;Animation
sound   = ZoneS1.snd   ;Sound
pal1    = ZoneS1.act  ;Palettes (can have up to 12)
pal2    = ZoneS1alt1.act ;Palettes 


