; Definition file for Robotnik S2 FB
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Robotnik S2 FB"        ;Name of character
displayname = "Metal Eggman S2" ;Name of character to display
versiondate = 07,05,2009    ;Version date of character (DD-MM-YYYY)
mugenversion = 04,14,2002   ;Version of M.U.G.E.N character works on
;Also works with Mugen 04,14,2001 and intermediate versions.
author = "supermystery"         ;Character author name

; Files for the player
[Files]
cmd     = RobotnikS2FZ.cmd.txt   ;Command set
cns     = RobotnikS2FZ.cns   ;Constants
st      = RobotnikS2FZ.cns   ;States
st2     = Armor.cns
st3     = eggS2.cns
st4     = AI.cns
stcommon = common1.cns ;Common states
sprite  = RobotnikS2FZ.sff   ;Sprite
anim    = RobotnikS2FZ.air.txt   ;Animation
sound   = RobotnikS2FZ.snd   ;Sound
pal1    = RobotnikS2FZnw.act  ;Palettes (can have up to 12)
pal2    = metaleggman.act
pal3    = RobotnikS2FZyel.act
pal4    = RobotnikS2FZpup.act  
pal5    = robotnikS2FZlime.act


