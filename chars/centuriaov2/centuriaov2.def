; Definition file for Centurion
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Centurion"        ;Name of character
displayname = "Centurion" ;Name of character to display
versiondate = 04,14,2001    ;Version date of character
mugenversion = 04,14,2001   ;Version of M.U.G.E.N character works on
author = "Sir Tigol"         ;Character author name
pal.defaults = 6,3,4,2      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = centuriao.cmd   ;Command set
cns     = centuriao.cns   ;Constants ;to play only with classic forms, switch to centuriao_classic.cns
st      = centuriao.cns   ;States    ;to play only with classic forms, switch to centuriao_classic.cns
stcommon = commonCentWin.cns ;common states
sprite  = centuriao.sff ;Sprites
anim    = centuriao.air   ;Animation
sound   = centuriao.snd   ;Sound
pal1    = centuriao.act  ;Palettes (can have up to 12)
pal2    = centgreen.act
pal3    = centplay2.act
pal4    = centblueblond.act
;pal5    = kfm3.act
;pal6    = kfm.act

; Arcade mode
[Arcade]
;intro.storyboard = intro.def
;ending.storyboard = ending.def