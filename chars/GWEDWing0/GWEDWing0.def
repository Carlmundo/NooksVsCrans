

; Player information
[Info]
name = "Wing Gundam Zero"        ;Name of character
displayname = "Wing.0" ;Name of character to display
versiondate = 1/1/2013    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Kamekaze"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = Wing.cmd           ;Command set
cns     = Wing.cns           ;Constants
st      = Wing.cns           ;States
st1 = Helper.cns
st2 = config.txt
stcommon = common.cns      ;Common states (from data/ or motif)
sprite  = Wing.sff           ;Sprite
anim    = Wing.air           ;Animation
sound   = Wing.snd           ;Sound
; Maps character selection buttons to palette numbers.
pal1    = Wing1.act
pal2    = Wing2.act
pal3    = Wing3.act
pal4    = Wing4.act
pal5    = Wing5.act
pal6    = Wing6.act

; Arcade mode
[Arcade]
intro.storyboard = intro.def
ending.storyboard = ending.def
