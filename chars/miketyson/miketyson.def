; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Mike Tyson"        ;Name of character
displayname = "Mike Tyson" ;Name of character to display
versiondate = 04,14,2002    ;Version date of character (MM-DD-YYYY)
mugenversion = 04,14,2002   ;Version of M.U.G.E.N character works on
author = "Bane84"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = miketyson.cmd   ;Command set
cns     = miketyson.cns   ;Constants
st      = miketyson.cns   ;States
stcommon = miketysoncommon.cns ;Common states
sprite  = miketyson.sff   ;Sprite
anim    = miketyson.air   ;Animation
sound   = miketyson.snd   ;Sound
pal1    = miketyson1.act

; Arcade mode
;[Arcade]
;intro.storyboard = intro.def
;ending.storyboard = ending.def
