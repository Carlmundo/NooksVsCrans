; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Samurai Jack"        ;Name of character
displayname = "Samurai Jack" ;Name of character to display
versiondate = 04,14,2001    ;Version date of character
mugenversion = 04,14,2001   ;Version of M.U.G.E.N character works on
author = "Pockefreeman"         ;Character author name
pal.defaults = 6,3,4,2      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = samurai_jack.cmd   ;Command set
cns     = samurai_jack.cns   ;Constants
st      = samurai_jack.cns   ;States
stcommon = common1.cns ;Common states
sprite  = samurai_jack.sff   ;Sprite
anim    = samurai_jack.air   ;Animation
sound   = samurai_jack.snd   ;Sound
pal1    = pal.act  ;Palettes (can have up to 12)
;pal2    = kfm4.act
;pal3    = kfm2.act
;pal4    = kfm5.act
;pal5    = kfm3.act
;pal6    = kfm.act

; Arcade mode
[Arcade]
;intro.storyboard = intro.def
;ending.storyboard = ending.def
